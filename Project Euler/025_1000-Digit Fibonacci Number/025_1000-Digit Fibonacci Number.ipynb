   "source": [
    "def fibonacci(length):\n",
    "    f_1 = 1\n",
    "    f_2 = 1\n",
    "    f_n = 0\n",
    "    index = 2\n",
    "    \n",
    "    while len(str(f_n)) < length:\n",
    "        f_n = f_1 + f_2\n",
    "        f_2 = f_1\n",
    "        f_1 = f_n\n",
    "        index += 1\n",
    "\n",
    "    return len(str(f_n)), index"
   ]
